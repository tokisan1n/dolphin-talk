{
  "manifest_version": 3,
  "name": "Dolphin Talk",
  "version": "0.2",
  "description": "ｷｭｲｰ",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js", "responses.js"],
      "css": ["style.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["dolphin.png"],
      "matches": ["<all_urls>"]
    }
  ]
}
